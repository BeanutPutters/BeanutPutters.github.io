    "# Customized fit function\n",
    "def objective(x, a, b, c):\n",
    "    return a * x + b * x**2 + c\n",
    "\n",
    "# Fitting the data\n",
    "popt, _ = curve_fit(objective, data[\"Followers\"], data[\"Average viewers\"])\n",
    "a, b, c = popt\n",
    "x_line = np.arange(min(data[\"Followers\"]), max(data[\"Followers\"]), 0.8)\n",
    "y_line = objective(x_line, a, b, c)\n",
    "\n",
